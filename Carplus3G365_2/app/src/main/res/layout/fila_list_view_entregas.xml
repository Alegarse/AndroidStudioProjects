<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linearLayout1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/fondodegradado"
    android:minHeight="45dip"
    android:orientation="horizontal"
    android:weightSum="1">

    <androidx.cardview.widget.CardView
        android:id="@+id/card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        app:cardCornerRadius="10dp"
        android:layout_margin="5dp"
        app:cardElevation="5dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:padding="10dp"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/linearLayout4"
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:background="@drawable/borde_blanco"
                android:gravity="top|start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtSucFilaEntregas"
                    android:textColor="@color/azulTitulo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:text="Suc"
                    android:textSize="12sp"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout5"
                android:layout_width="45dp"
                android:layout_height="fill_parent"
                android:background="@drawable/borde_blanco"
                android:gravity="top|left"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtHoraFilaEntregas"
                    android:textColor="@color/azulTitulo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:text="@string/hour"
                    android:textSize="12sp"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout6"
                android:layout_width="30dp"
                android:layout_height="fill_parent"
                android:background="@drawable/borde_blanco"
                android:gravity="top|start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtGpFilaEntregas"
                    android:textColor="@color/azulTitulo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:text="Gp"
                    android:textSize="12sp"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout7"
                android:layout_width="30dp"
                android:layout_height="fill_parent"
                android:background="@drawable/borde_blanco"
                android:gravity="top|start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtGp2FilaEntregas"
                    android:textColor="@color/azulTitulo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:text="Gp2"
                    android:textSize="12sp"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout8"
                android:layout_width="40dp"
                android:layout_height="fill_parent"
                android:background="@drawable/borde_blanco"
                android:gravity="top|left"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtDiasFilaEntregas"
                    android:textColor="@color/azulTitulo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:text="@string/days"
                    android:textSize="12sp"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout9"
                android:layout_width="60dp"
                android:layout_height="fill_parent"
                android:background="@drawable/borde_blanco"
                android:gravity="top|start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtMatriculaFilaEntregas"
                    android:textColor="@color/azulTitulo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:text="@string/car_plate"
                    android:textSize="12sp"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/txtEstadoFila"
                android:layout_width="90dp"
                android:layout_height="fill_parent"
                android:background="@drawable/borde_blanco"
                android:gravity="top|start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtClienteFilaEntregas"
                    android:textColor="@color/azulTitulo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:text="@string/client"
                    android:maxLines="2"
                    android:textSize="12sp"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout02"
                android:layout_width="90dp"
                android:layout_height="fill_parent"
                android:background="@drawable/borde_blanco"
                android:gravity="top|start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtLugarFilaEntregas"
                    android:textColor="@color/azulTitulo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:text="@string/collect_site"
                    android:maxLines="1"
                    android:textSize="12sp"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout01"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@drawable/borde_blanco"
                android:gravity="top|start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtObsFilaEntregas"
                    android:textColor="@color/azulTitulo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:text="obs"
                    android:textSize="12sp"/>
            </LinearLayout>


        </LinearLayout>
    </androidx.cardview.widget.CardView>



</LinearLayout>
