<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondodegradado"
    tools:context=".Checkin">


    <ImageButton
        android:id="@+id/salirCH"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/boton_base_rojo"
        android:gravity="center"
        android:src="@drawable/exit_session"
        android:tint="#fff"
        android:layout_margin="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/btnNuevoVehiculoEstado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:backgroundTint="@color/colorAzulBtn"
        android:onClick="btnNuevoVehiculoCheckinClick"
        android:text="@string/new_veh"
        app:layout_constraintEnd_toStartOf="@+id/salirCH"
        app:layout_constraintStart_toEndOf="@+id/menuPpalCH"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/menuPpalCH"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_margin="10dp"
        android:background="@drawable/boton_base_verde"
        android:gravity="center"
        android:src="@drawable/go_home"
        android:tint="#fff"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <LinearLayout
        android:id="@+id/content_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="65dp"
        android:visibility="visible"
        tools:ignore="MissingConstraints">

        <ScrollView
            android:id="@+id/scrollCheckin"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="70dp"
            android:scrollbars="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <!-- CardView Ficha del vehículo -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardCheckin1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginBottom="0dp"
                    android:backgroundTint="#FFFFFF"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <!-- Titulo ficha vehiculo -->
                        <LinearLayout
                            android:id="@+id/LinearCabeceraFicVeh"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:layout_margin="5dp"
                            android:background="@drawable/degradado_azul_linear_layout"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tituloFicVeh"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center_vertical"
                                android:text="@string/title_card_vehicle"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#fff"
                                android:textSize="17sp"/>

                            <LinearLayout
                                android:id="@+id/linearDentroMatricula"
                                android:layout_width="200dip"
                                android:layout_height="fill_parent"
                                android:layout_marginLeft="10dip"
                                android:layout_marginTop="2dip"
                                android:layout_marginBottom="2dip"
                                android:background="@drawable/matricula"
                                android:gravity="center"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/txtMatriculaCheckin"
                                    style="@style/texto_negro"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/common_card_plate"
                                    android:textSize="22sp"/>
                            </LinearLayout>

                        </LinearLayout>

                        <TableLayout
                            android:id="@+id/TableLayout01"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:padding="5dip"
                            android:shrinkColumns="1"
                            android:stretchColumns="1">

                            <TableRow>

                                <LinearLayout
                                    android:id="@+id/LinearLayout08"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/TextView03"
                                        style="@style/textoAzuladoCard"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/document"/>

                                    <TextView
                                        android:id="@+id/txtDocumentoCheckin"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="#333333"/>
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/LinearLayout01"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/TextView15"
                                        style="@style/textoAzuladoCard"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/depositary"/>

                                    <TextView
                                        android:id="@+id/txtDepositarioCheckin"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="#333333"/>
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/LinearLayout02"
                                    android:layout_width="140dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/TextView05"
                                        style="@style/textoAzuladoCard"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/delivery_date"/>

                                    <TextView
                                        android:id="@+id/txtF1Checkin"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="#333333" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/LinearLayout03"
                                    android:layout_width="140dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/TextView06"
                                        style="@style/textoAzuladoCard"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/recovery_date"/>

                                    <TextView
                                        android:id="@+id/txtF2Checkin"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:width="150dp"
                                        android:textColor="#333333" />
                                </LinearLayout>

                            </TableRow>
                        </TableLayout>


                    </LinearLayout>



                </androidx.cardview.widget.CardView>

                <!-- CardView Información del vehículo -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardCheckin2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginBottom="0dp"
                    android:backgroundTint="#FFFFFF"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <!-- Titulo info vehículo -->
                        <LinearLayout
                            android:id="@+id/LinearCabeceraInfVeh"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:layout_margin="5dp"
                            android:background="@drawable/degradado_azul_linear_layout"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tituloInfVeh"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center_vertical"
                                android:text="@string/veh_info"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#fff"
                                android:textSize="17sp"/>

                        </LinearLayout>

                        <TableLayout
                            android:id="@+id/tableLayout1"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:padding="5dip"
                            android:shrinkColumns="2"
                            android:stretchColumns="2">

                            <TableRow>

                                <LinearLayout
                                    android:id="@+id/LinearLayout04"
                                    android:layout_width="160dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/TextView07"
                                        style="@style/textoAzuladoCard"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/fuel_octaves"/>

                                    <Spinner
                                        android:id="@+id/spinOctavos"
                                        android:layout_width="160dip"
                                        android:layout_height="wrap_content"/>
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/LinearLayout07"
                                    android:layout_width="150dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/TextView10"
                                        style="@style/textoAzuladoCard"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/kilometers"/>

                                    <EditText
                                        android:id="@+id/txtKms"
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content"
                                        android:inputType="number"
                                        android:maxLength="6"
                                        android:selectAllOnFocus="true"/>
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/LinearLayout05"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/TextView13"
                                        style="@style/textoAzuladoCard"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/color"/>

                                    <TextView
                                        android:id="@+id/txtColorCheckin"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="#333333"/>
                                </LinearLayout>
                            </TableRow>
                        </TableLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <!-- CardView Comprobación equipamiento -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardCheckin3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginBottom="0dp"
                    android:backgroundTint="#FFFFFF"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <!-- Titulo info vehículo -->
                        <LinearLayout
                            android:id="@+id/LinearCabeceraCompEq"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:layout_margin="5dp"
                            android:background="@drawable/degradado_azul_linear_layout"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tituloCompEq"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center_vertical"
                                android:text="@string/check_equipment"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#fff"
                                android:textSize="17sp"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linealEquipamiento"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:padding="5dp">


                            <Button
                                android:id="@+id/botonAntena"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="5dp"
                                android:backgroundTint="@color/colorGrisBtn"
                                android:textColor="#fff"
                                android:text="@string/antenna"
                                android:clickable="false"
                                style="@style/Widget.MaterialComponents.Button.TextButton"/>
                            <Button
                                android:id="@+id/botonChaleco"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="5dp"
                                android:backgroundTint="@color/colorGrisBtn"
                                android:textColor="#fff"
                                android:text="@string/vest"
                                android:clickable="false"
                                style="@style/Widget.MaterialComponents.Button.TextButton"/>
                            <Button
                                android:id="@+id/botonRueda"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="5dp"
                                android:backgroundTint="@color/colorGrisBtn"
                                android:textColor="#fff"
                                android:text="@string/wheel"
                                android:clickable="false"
                                style="@style/Widget.MaterialComponents.Button.TextButton"/>
                            <Button
                                android:id="@+id/botonGato"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="5dp"
                                android:backgroundTint="@color/colorGrisBtn"
                                android:textColor="#fff"
                                android:text="@string/car_jack"
                                android:clickable="false"
                                style="@style/Widget.MaterialComponents.Button.TextButton"/>
                            <Button
                                android:id="@+id/botonTriangulo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:backgroundTint="@color/colorGrisBtn"
                                android:textColor="#fff"
                                android:text="@string/triangle"
                                android:clickable="false"
                                style="@style/Widget.MaterialComponents.Button.TextButton"/>

                            <Button
                                android:id="@+id/botonBaca"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:backgroundTint="@color/colorGrisBtn"
                                android:textColor="#fff"
                                android:text="@string/car_roof_rack"
                                android:clickable="false"
                                style="@style/Widget.MaterialComponents.Button.TextButton"/>


                        </LinearLayout>
                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <!-- CardView Comprobación extras -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardCheckin4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginBottom="0dp"
                    android:backgroundTint="#FFFFFF"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <!-- Titulo info vehículo -->
                        <LinearLayout
                            android:id="@+id/LinearCabeceraExtVeh"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:layout_margin="5dp"
                            android:background="@drawable/degradado_azul_linear_layout"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tituloExtVeh"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center_vertical"
                                android:text="@string/check_extras"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#fff"
                                android:textSize="17sp"/>

                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/contExtrasCheckin"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="5dp"
                            android:focusableInTouchMode="true"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent">

                        </LinearLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <!-- CardView Comprobación daños -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardCheckin5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginBottom="0dp"
                    android:backgroundTint="#FFFFFF"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <!-- Titulo info vehículo -->
                        <LinearLayout
                            android:id="@+id/LinearCabeceraDaVeh"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:layout_margin="5dp"
                            android:background="@drawable/degradado_azul_linear_layout"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center_vertical">

                            <TextView
                                android:id="@+id/tituloDaVeh"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center_vertical"
                                android:text="@string/check_damage"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#fff"
                                android:textSize="17sp"/>
                            <Button
                                android:id="@+id/btnAddExtra"
                                android:layout_width="wrap_content"
                                android:layout_height="35dp"
                                android:layout_centerVertical="true"
                                android:layout_alignParentEnd="true"
                                android:paddingVertical="1dp"
                                android:layout_marginEnd="5dp"
                                android:onClick="btnAddDanosClick"
                                android:background="@drawable/boton_base_redondeado"
                                app:backgroundTint="@color/amarilloPalido"
                                android:text="@string/add_damage" />

                            </RelativeLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayout4"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/fondo_blanco_borde_gris"
                            android:gravity="top">

                            <LinearLayout
                                android:id="@+id/LinearLayout14"
                                android:layout_width="453dp"
                                android:layout_height="201dp"
                                android:background="@drawable/coche"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:id="@+id/RelativeLayout01"
                                    android:layout_width="453dp"
                                    android:layout_height="201dp">

                                    <ImageView
                                        android:id="@+id/pic01y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="15dp"
                                        android:layout_marginTop="30dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic02y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="15dp"
                                        android:layout_marginTop="160dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic03y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="75dp"
                                        android:layout_marginTop="15dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic04y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="75dp"
                                        android:layout_marginTop="170dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic05y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="55dp"
                                        android:layout_marginTop="95dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic06y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="120dp"
                                        android:layout_marginTop="10dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic07y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="120dp"
                                        android:layout_marginTop="170dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic08y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="190dp"
                                        android:layout_marginTop="10dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic09y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="190dp"
                                        android:layout_marginTop="170dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic10y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="215dp"
                                        android:layout_marginTop="30dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic11y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="215dp"
                                        android:layout_marginTop="152dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic12y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="150dp"
                                        android:layout_marginTop="95dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic13y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="260dp"
                                        android:layout_marginTop="95dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic14y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="290dp"
                                        android:layout_marginTop="10dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic15y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="270dp"
                                        android:layout_marginTop="30dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic16y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="270dp"
                                        android:layout_marginTop="152dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic17y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="290dp"
                                        android:layout_marginTop="170dp"
                                        android:src="@drawable/yellowstar"/>

                                    <ImageView
                                        android:id="@+id/pic18y"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="365dp"
                                        android:layout_marginTop="95dp"
